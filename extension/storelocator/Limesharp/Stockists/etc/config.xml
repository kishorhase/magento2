                <radius>40233.6</radius>
                <map_style>default</map_style>
                <ask_location>1</ask_location>
            </stockist_map>    
            <stockist_radius>
                <circle_stroke_weight>1</circle_stroke_weight>
                <circle_stroke_opacity>0.5</circle_stroke_opacity>
                <circle_stroke_color>#0000A0</circle_stroke_color>
                <circle_fill_opacity>0.2</circle_fill_opacity>
                <circle_fill_color>#00FFFF</circle_fill_color>
            </stockist_radius>
            <stockist_individual>
                <zoom_individual>16</zoom_individual>
                <stores_slider>1</stores_slider>
                <other_stores>1</other_stores>
            </stockist_individual>
