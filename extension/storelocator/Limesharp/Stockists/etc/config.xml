<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <limesharp_stockists>
            <stockist_content>
                <breadcrumbs>1</breadcrumbs>
                <meta_title>Stockists</meta_title>
                <meta_description>Stockists Description Here</meta_description>
                <meta_keywords>Stockists Keywords Here</meta_keywords>
                <url>stockists</url>
            </stockist_content>
            <stockist_map>
                <template>full_width_sidebar</template>
                <api_key>AIzaSyDQoYGuJE3CogwzmFTzp7Eb8LpL5Tp-swo</api_key>
                <unit_length>default</unit_length>
                <zoom>13</zoom>
